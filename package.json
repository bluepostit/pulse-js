{
  "name": "pulse",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "dev": "dotenv -e .env.development -- ts-node src/index.ts",
    "dev:migrate": "dotenv -e .env.development -- npx prisma migrate dev --name",
    "dev:generate": "dotenv -e .env.development -- npx prisma generate",
    "docker:up": "sudo docker-compose up -d",
    "docker:down": "sudo docker-compose down",
    "test:migrate": "dotenv -e .env.test -- npx prisma migrate dev",
    "test:jest": "dotenv -e .env.test -- jest",
    "test:jest:watch": "dotenv -e .env.test -- jest --watch",
    "test:watch": "npm run docker:up && npm run test:migrate && npm run test:jest:watch",
    "test": "npm run docker:up && npm run test:migrate && npm run test:jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.6.1",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "babel-jest": "^29.3.1",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.2",
    "jest": "^29.2.2",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "prisma": "^4.6.1",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
